{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "AppDbContext": "Data Source=app.db"
  },
  "AWS": {
    "AccessKey": "YOUR_ACCESS_KEY",
    "SecretKey": "YOUR_SECRET_KEY",
    "Region": "eu-west-1",
    "SourceEmail": "noreply@yourdomain.com",
    "UseDmarc": true,
    "BaseUrl": "https://yourdomain.com"
  },
  "AwsSsm": {
    "Region": "central-1",
    "UsernameParam": "/autodokasapp/smtp/username",
    "PasswordParam": "/autodokasapp/smtp/password",
    "HostParam": "/autodokasapp/smtp/host",
    "PortParam": "/autodokasapp/smtp/port",
    "DecryptParameters": true
  },
  "AllowedHosts": "*"
}
